{
    "name": "Genomics",
    "version": "1.2.0",
    "author": "Shree Swami Samarth Media Pvt Ltd",
    "description": "Genomics Admin App",
    "main": "app.js",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "package": "electron-packager . MyAngularApp --platform=win32 --arch=x64 --out=release --overwrite",
        "dist": "ng build --configuration=production --aot --base-href ./ && electron-builder --win",
        "build:electron": "ng build --configuration=production --aot --base-href ./ && electron-packager ./dist/sakai-ng my-electron-app --platform=win32 --arch=x64 --out=release --overwrite",
        "electron": "ng build --configuration=production --aot --base-href ./ && electron ."
    },
    "build": {
        "appId": "com.example.app",
        "productName": "Genomics Admin",
        "directories": {
            "output": "dist"
        },
        "files": [
            "dist/**/*",
            "node_modules/**/*",
            "app.js",
            "package.json"
        ],
        "win": {
            "target": "nsis",
            "icon": "src/assets/layout/images/logo.ico"
        },
        "nsis": {
            "oneClick": false,
            "perMachine": true,
            "allowElevation": true,
            "allowToChangeInstallationDirectory": true
        }
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^17.0.5",
        "@angular/cdk": "^17.0.2",
        "@angular/common": "^17.0.5",
        "@angular/compiler": "^17.0.5",
        "@angular/core": "^17.0.5",
        "@angular/forms": "^17.0.5",
        "@angular/platform-browser": "^17.0.5",
        "@angular/platform-browser-dynamic": "^17.0.5",
        "@angular/router": "^17.0.5",
        "@electron/remote": "^2.1.2",
        "@fullcalendar/angular": "^6.0.3",
        "@fullcalendar/core": "^6.0.3",
        "@fullcalendar/daygrid": "^6.0.3",
        "@fullcalendar/interaction": "^6.0.3",
        "@fullcalendar/timegrid": "^6.0.3",
        "chart.js": "^3.3.2",
        "file-saver": "^2.0.5",
        "html2canvas": "^1.4.1",
        "html2pdf": "^0.0.11",
        "html2pdf.js": "^0.10.2",
        "jspdf": "^2.5.1",
        "jspdf-autotable": "^3.8.3",
        "primeflex": "^3.3.1",
        "primeicons": "6.0.1",
        "primeng": "17.2.0",
        "print-js": "^1.6.0",
        "prismjs": "^1.29.0",
        "quill": "^1.3.7",
        "rxjs": "~7.8.1",
        "tslib": "^2.5.0",
        "xlsx": "^0.18.5",
        "zone.js": "~0.14.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^17.0.5",
        "@angular/cli": "^17.0.5",
        "@angular/compiler-cli": "^17.0.5",
        "@electron/packager": "^18.3.3",
        "@electron/windows-sign": "^1.1.3",
        "@types/jasmine": "~4.3.1",
        "@types/jspdf": "^2.0.0",
        "@types/node": "^22.2.0",
        "@types/xlsx": "^0.0.36",
        "cross-dirname": "^0.1.0",
        "electron": "^31.3.1",
        "electron-builder": "^24.13.3",
        "electron-log": "^5.2.3",
        "electron-packager": "^17.1.2",
        "fs-extra": "^11.2.0",
        "jasmine-core": "~4.6.0",
        "karma": "~6.4.2",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "node-fetch": "^3.3.2",
        "typescript": "~5.2.2"
    }
}
